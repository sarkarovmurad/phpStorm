<application>
  <component name="MessDetector">
    <phpmd_settings_migrated />
  </component>
  <component name="PhpCodeSniffer">
    <phpcs_settings_migrated />
    <phpcs_settings>
      <PhpCSConfiguration max_messages_per_file="100" standards="MySource;PEAR;PSR1;PSR2;Squiz;Webpractik;Zend" timeout="2000" tool_path="$USER_HOME$/.config/composer/vendor/bin/phpcs" />
    </phpcs_settings>
  </component>
</application>